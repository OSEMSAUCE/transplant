{
	"name": "transplant",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"db:pull:dev": "dotenv -e .env.dev -- prisma db pull",
		"db:pull:prod": "dotenv -e .env.production -- prisma db pull",
		"db:generate:prod": "dotenv -e .env.production -- prisma generate",
		"db:sync:prod": "clear && dotenv -e .env.production -- prisma db pull && prisma generate",
		"db:generate:dev": "prisma generate",
		"dev:prod": "clear && dotenv -e .env.production vite dev --watch",
		"dev:prod_db": "dotenv -e .env.production vite dev",
		"import:dev": "dotenv -e .env.dev node src/routes/api/submitGeoJson/importGeoJson.cjs",
		"import:prod": "dotenv -e .env.production node src/routes/api/submitGeoJson/importGeoJson.cjs",
		"migrate:dev": "dotenv -e .env.dev prisma migrate dev",
		"migrate:prod": "dotenv -e .env.production prisma migrate deploy",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"db:reset:dev": "dotenv -e .env.dev prisma migrate reset",
		"deploy": "gh-pages -d build"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.17.2",
		"@sveltejs/vite-plugin-svelte": "^5.0.0-next.0",
		"@types/papaparse": "^5.3.15",
		"concurrently": "^9.1.2",
		"dotenv-cli": "^8.0.0",
		"esbuild": "^0.25.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-svelte": "^2.46.1",
		"gh-pages": "^6.3.0",
		"globals": "^15.14.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prisma": "^6.11.0",
		"sass-embedded": "^1.85.0",
		"svelte": "^5.20.4",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.0.0"
	},
	"dependencies": {
		"@picocss/pico": "^2.0.6",
		"@prisma/client": "^6.9.0",
		"@supabase/auth-helpers-sveltekit": "^0.13.0",
		"@supabase/ssr": "^0.6.1",
		"@supabase/supabase-js": "^2.50.0",
		"@sveltejs/adapter-vercel": "^5.7.2",
		"papaparse": "^5.5.2",
		"zod": "^3.25.76"
	}
}
