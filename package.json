    "@swc/cli": "^0.4.1-nightly.20240914",
    "@swc/core": "^1.7.26",
    "@tanstack/svelte-table": "^8.20.5",
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "eslint": "^9.10.0",
    "eslint-plugin-svelte": "^2.43.0",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "prisma": "^5.18.0",
    "sass": "^1.77.8",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "svelte-preprocess": "^6.0.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vite": "^5.0.3",
    "vitest": "^2.0.0"
