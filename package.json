{
	"name": "transplant",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"db:generate:dev": "prisma generate",
		"db:generate:prod": "dotenv -e .env.production -- prisma generate",
		"db:pull:dev": "dotenv -e .env.dev -- prisma db pull",
		"db:pull:prod": "dotenv -e .env.production -- prisma db pull",
		"dev": "dotenv -e .env.dev -- vite dev",
		"dev:prod_db": "dotenv -e .env.production -- vite dev",
		"import:dev": "dotenv -e .env.dev -- node src/routes/api/submitGeoJson/importGeoJson.cjs",
		"import:prod": "dotenv -e .env.production -- node src/routes/api/submitGeoJson/importGeoJson.cjs",
		"migrate:dev": "dotenv -e .env.dev -- prisma migrate dev",
		
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.17.2",
		"@sveltejs/vite-plugin-svelte": "^5.0.0-next.0",
		"@types/papaparse": "^5.3.15",
		"dotenv-cli": "^8.0.0",
		"esbuild": "^0.25.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prisma": "^6.7.0",
		"sass-embedded": "^1.85.0",
		"svelte": "^5.20.4",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.0.0"
	},
	"dependencies": {
		"@picocss/pico": "^2.0.6",
		"@prisma/client": "^6.7.0",
		"@sveltejs/adapter-vercel": "^5.6.3",
		"papaparse": "^5.5.2"
	}
}
