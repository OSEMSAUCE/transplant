  "private": true,
  "scripts": {
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "dev": "vite dev",
    "eslint": "eslint . --ext .js,.ts,.svelte",
    "eslint:fix": "eslint . --ext .js,.ts,.svelte --fix",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint . --ext .js,.ts,.svelte",
    "lint:fix": "prettier --write . && eslint . --ext .js,.ts,.svelte --fix",
    "preview": "vite preview",
    "seed": "npx ts-node --esm prisma/seed.ts",
    "start": "vite preview --port $PORT",
    "test": "npm run test:integration && npm run test:unit",
    "test:integration": "playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
